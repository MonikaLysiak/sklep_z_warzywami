@model IEnumerable<SklepZWarzywami.Models.DbModels.ZakupJednostkowy>

@{
    ViewBag.Title = "ListaZakupow";
}

<h2>Index</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Warzywo.Nazwa)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Zakup.ZakupId)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Waga)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Cena)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Warzywo.Nazwa)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Zakup.ZakupId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Waga)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Cena)
            </td>
        </tr>
    }

</table>
